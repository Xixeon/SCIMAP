<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.user.sci_map">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity
                android:name=".SpalshActivity"
                android:label="@string/app_name"
                android:noHistory="true"
                android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
                android:name=".MainActivity"
                android:label="@string/app_name"
                android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity
                android:name=".building1"
                android:label="@string/title_activity_building1">
        </activity>
        <activity
                android:name=".phrajom"
                android:label="@string/title_activity_phrajom"
                android:parentActivityName=".MainActivity">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="com.example.user.sci_map.MainActivity"/>
        </activity>
        <activity android:name=".Phrajom">
        </activity>
        <activity android:name=".InPhrajom">
        </activity>
        <activity android:name=".Phrajom1">
        </activity>
    </application>

</manifest>